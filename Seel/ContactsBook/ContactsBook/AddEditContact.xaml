<Window x:Class="ContactsBook.AddEditContact"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ContactsBook"
        mc:Ignorable="d"
        Title="AddEditContact" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="46,25,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="171"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="46,59,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="171"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="23" Margin="46,95,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="171"/>
        <Button x:Name="button" Content="Add new" HorizontalAlignment="Left" Height="55" Margin="54,139,0,0" VerticalAlignment="Top" Width="149" Click="button_Click"/>
        <Button x:Name="button1" Content="Edit contact" HorizontalAlignment="Left" Height="55" Margin="54,199,0,0" VerticalAlignment="Top" Width="149" Click="button1_Click"/>
    </Grid>
</Window>
